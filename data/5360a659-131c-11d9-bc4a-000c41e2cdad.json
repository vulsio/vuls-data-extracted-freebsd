	"advisories": [
		{
			"content": {
				"id": "5360a659-131c-11d9-bc4a-000c41e2cdad",
				"title": "mozilla -- hostname spoofing bug",
				"description": "\n\t<p>When processing URIs that contain an unqualified host name--\n\t  specifically, a domain name of only one component--\n\t  Mozilla will perform matching against the first component\n\t  of the domain name in SSL certificates.  In other words, in\n\t  some situations, a certificate issued to \"www.example.com\"\n\t  will be accepted as matching \"www\".</p>\n      ",
				"references": [
					{
						"source": "vuxml.freebsd.org",
						"url": "http://bugzilla.mozilla.org/show_bug.cgi?id=234058"
					},
					{
						"source": "vuxml.freebsd.org",
						"url": "https://www.vuxml.org/freebsd/5360a659-131c-11d9-bc4a-000c41e2cdad.html"
					}
				],
				"published": "2004-09-30T00:00:00Z"
			},
